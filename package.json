{
  "name": "vite-plugin-electron-builder",
  "version": "0.0.0",
  "license": "MIT",
  "author": "M2",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsc -p . -w --incremental",
    "build": "rimraf dist && run-s build-bundle build-types",
    "build-bundle": "esbuild src/index.ts --bundle --platform=node --target=node12 --outfile=dist/index.js",
    "build-types": "tsc -p . --emitDeclarationOnly --outDir temp && rimraf temp"
  },
  "keywords": [],
  "devDependencies": {
    "chalk": "^4.1.2",
    "electron": "^13.1.9",
    "electron-builder": "^22.11.7",
    "esbuild": "^0.12.20",
    "fs-extra": "^8.1.0"
  }
}
